@model CalendarApp.Models.Event

<h2>Event Details</h2>

<p><strong>Title:</strong> @Model.Title</p>
<p><strong>Start Time:</strong> @Model.StartTime.ToString("f")</p>
<p><strong>End Time:</strong> @Model.EndTime.ToString("f")</p>
<p><strong>Description:</strong> @Model.Description</p>

<a href="/event">Back to Calendar</a>
